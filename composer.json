{
  "name": "karlosagudo/fixtro",
  "type": "library",
  "description": "A QA static analysis code",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "karlosagudo",
      "email": "karlosagudo1978+fixtro@gmail.com",
      "role": "Developer"

    }
  ],
  "support": {
    "email": "karlosagudo1978+fixtro@gmail.com"
  },
  "require": {
    "php":"^7.0",
    "symfony/console": "^2.0 || ^3.0 || ^4.0",
    "symfony/process": "^2.0 || ^3.0 || ^4.0",
    "symfony/yaml": "^2.0 || ^3.0 || ^4.0",
    "symfony/event-dispatcher": "^2.0 || ^3.0 || ^4.0",
    "symfony/finder":  "^2.0 || ^3.0 || ^4.0",
    "behat/behat": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0",
    "jakub-onderka/php-parallel-lint": "^0.9.2 || ^1.0",
    "friendsofphp/php-cs-fixer": "^2.1",
    "phpmd/phpmd": "@stable",
    "sensiolabs/security-checker": "^4.0",
    "vimeo/psalm": "dev-master",
    "phpstan/phpstan": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "prefer-stable": true,
  "bin": [
     "bin/fixtro", "bin/fixtro-precommit", "bin/php_no_xdebug"
  ],
  "autoload": {
    "psr-4": {
      "KarlosAgudo\\Fixtro\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KarlosAgudo\\Fixtro\\Tests\\": "tests/"
    }
  }
}
